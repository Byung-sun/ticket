    <script>

        var x = [];
        var y = [];
        var time = [];

        
        var t1;
        var t2;

        function info(concert, name){
            console.log("info suc")
            console.log("index_1 name = ",name)
            location.href = "/concert/index?concert="+concert+"&x="+x+"&y="+y+"&time="+time+"&name="+name;
        }
        // var click_list = [];

        jQuery(document).ready(function(){
            $(document).mousedown(function(e){      //마우스 클릭시 click에 좌표 입력
                var tt = new Date().getTime();
                
                if(!t1){
                    x.push(e.pageX);
                    y.push(e.pageY);
                    time.push("0");
                    console.log("x = ", x , "y =", y, "time = 0");
                    t1= tt;
                    console.log(t1);
                }else{
                    t2 = tt;
                    console.log("t2 = ",t2)
                    console.log("t2-t1 = ",t2-t1)
                    x.push(e.pageX);
                    y.push(e.pageY);
                    time.push(t2-t1);
                    console.log("x = ", x , "y =", y, "time = ", time);
                    t1= t2;
                }

                // $.post("click", {x: e.pageX, y: e.pageY});
            });
        })
    </script>

